    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "semver": "^7.5.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "@types/semver": "^7.3.12",
    "@vercel/ncc": "^0.34.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
