{
  "name": "setup-raku",
  "version": "1.0.0",
  "private": true,
  "description": "setup raku for github actions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --noEmit && esbuild src/setup-raku.ts --bundle --platform=node --format=cjs --target=node24 --outfile=dist/index.js",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "vitest run",
    "pre-checkin": "npm run format && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raku/setup-raku.git"
  },
  "keywords": [
    "actions",
    "raku",
    "setup"
  ],
  "author": "Raku community",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/http-client": "^4.0.0",
    "@actions/io": "^3.0.2",
    "@actions/tool-cache": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.11.0",
    "esbuild": "^0.27.3",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=24"
  }
}
